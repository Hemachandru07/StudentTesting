@model IEnumerable<StudentTesting.Models.Marks>

@{
    ViewData["Title"] = "Index";
    Layout = "Teacher_layout";
}

<h4 style="text-align:center">Result Page</h4>

<p style="text-align:center">
    <a class="btn btn-primary" asp-action="Create">Update Result</a>
</p>


<table class="table" style="color:black;text-align:center;font-size:20px">
    <thead>
        <tr>
            
            <th>
                @Html.DisplayNameFor(model => model.Studentid.StudentName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Studentid.Reg_no)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Subject1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Subject2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Subject3)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Subject4)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Subject5)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Subject6)
            </th>
            
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            
             <td>
                @Html.DisplayFor(modelItem => item.Studentid.StudentName)
            </td>
             <td>
                @Html.DisplayFor(modelItem => item.Studentid.Reg_no)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Subject1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Subject2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Subject3)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Subject4)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Subject5)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Subject6)
            </td>
           
            @*<td>
                <a asp-action="Edit" asp-route-id="@item.MarksId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MarksId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.MarksId">Delete</a>
            </td>*@
        </tr>
}
    </tbody>
</table>
